{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "./entitlements.mac.plist",
      "minimumSystemVersion": "14.0"
    },
    "targets": "all"
  },
  "productName": "Stories",
  "version": "0.0.0",
  "identifier": "com.stories.app",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "titleBarStyle": "Visible",
        "fullscreen": false,
        "resizable": true,
        "title": "Stories",
        "minWidth": 1280,
        "minHeight": 800,
        "center": true,
        "closable": true,
        "hiddenTitle": true,
        "acceptFirstMouse": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
